/* CSS Document */

body{ background:#ffffff;}
.news_list_top{ width:96%; margin:10px auto 0px;}
.news_list_container{ width:96%; margin:0px auto;}
.news_list_top h4{height: 32px;line-height: 32px;border-bottom: 1px solid gainsboro;font-size: 12px;color: #000;font-weight: 700;}
.news_list_ul li{display:block; background:url(../images/icon-news.gif) 0px 0px no-repeat; line-height:18px;border-bottom:1px dotted #ccc; padding-left:20px; position:relative;}
.news_list_ul li a{display:block; width:100%;}
.news_time{color:#999; padding-right:10px;line-height:30px;}
.news_name{color:#000;}
.news_list_right{position:absolute; right:10px; top:3px; line-height:30px;}