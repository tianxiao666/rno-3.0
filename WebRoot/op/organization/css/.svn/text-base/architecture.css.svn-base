.dialog_pe_table .menu_td { background: none repeat scroll 0 0 #E8EDFF; text-align: right; white-space: nowrap; width: 70px;border-right:1px solid #CCCCCC;}
.dialog_pe_table .menu_td_l {border-left:1px solid #CCCCCC}
.dialog_pe_table tr td { border: 1px solid #CCCCCC; color: #000000; line-height: 25px; padding: 6px; vertical-align: middle; white-space: nowrap; }
.dialog_pe_table .w_305{width:305px;}
.dialog_pe_table .w_160{width:160px;}
.dialog_pe_table .w_120{width:120px;}
.dialog_pe_table .w_90{width:90px;}

.jineng_table{width:100%}
.dialog_pe_table .jineng_table tr td {border:none;}
.show_treeview_div{background: none repeat scroll 0 0 #F0F0F0; border: 1px solid #CCCCCC; margin-top: 1px; position: absolute; width: 372px;}
.show_treeview_info{overflow:auto;height: 194px;}
.show_treeview_but, .show_architecture_but{border-top: 1px solid #CCCCCC; padding: 5px; text-align: center;}
.show_treeview_but input, .show_architecture_but input{margin:0 3px;}

/*赋予角色*/
.show_architecture_div{background: none repeat scroll 0 0 #F0F0F0; border: 1px solid #CCCCCC; position: absolute;margin-top: -154px; width: 429px;}
.show_architecture_info{height: 277px;}
.architecture_info_title{border-bottom: 1px solid #CCCCCC;background: none repeat scroll 0 0 #DBDBDB;}
.architecture_info_title li{float:left;text-align: center; width: 142px;}
.architecture_info_org, .architecture_info_business, .architecture_info_role{float: left; height: 251px; overflow: auto; text-align: center; width: 142px;}
.architecture_info_org, .architecture_info_business{border-right: 1px solid #CCCCCC;}
.architecture_info_org li, .architecture_info_business li, .architecture_info_role li{border-bottom: 1px dashed #CCCCCC; line-height: 30px; cursor: pointer;}
.architecture_info_org li:hover, .architecture_info_business li:hover, .architecture_info_role li:hover{background-color:#dbdbdb;}
.architecture_info_org li:last-child, .architecture_info_business li:last-child, .architecture_info_role li:last-child {border-bottom: none;}
.architecture_info_role  li input{margin:0; padding:0;}
.architecture_info_role  li span{margin-left:5px;}

.role{float:left; width:500px;}
.role .tab_menu{clear: both;height: 25px;}
.role .tab_menu ul li {background-color:#EEEEEE; border-right: 1px solid #ccc; border-top: 1px solid #ccc;}
.role .tab_menu ul li.selected {background-color:#FFFFFF; border-right: 1px solid #ccc; border-top: 3px solid #aaa;}
.skill, .endow_role_info{border: 1px solid #CCCCCC;}
.role_title{background-color: #F0F0F0;border-bottom: 1px solid #CCCCCC;}
.endow_role_info .role_title{margin-top:5px; border-top: 1px solid #CCCCCC;}
.role_table { width: 100%; max-height: 95px;overflow: auto;}
.role_table table{ width: 100%;}
.role_table tr{border-bottom: 1px dashed #CCCCCC;}
.role_table tr:last-child {border-bottom: none;}
.role_table td{border:none; text-align: center; width: 30%;padding: 5px; }
.endow_role_info .role_table td{border:none; text-align: center; width: 50%;padding: 5px; }
.role_table .role_table_tr, .endow_role_info .role_table .role_table_tr{border-right: 1px dashed #CCCCCC;}
.role_info, .role_info_s{height: 80px;overflow: auto;}
.role_info span, .role_title span { display: inline-block; text-align: center;  vertical-align: middle; width: 49%;margin-left: 0px; padding: 0px;line-height: 26px;}
.role_no{text-align: center;}
.role_a{display: block; padding-top: 5px; text-align: center; cursor:pointer;}
.skill_a{display: block; padding: 0 10px 10px 0; text-align: right; cursor:pointer;}
.role_but{padding-top: 5px;text-align: center;}
.role_but input{margin:0 3px;}
#calendardiv input, #calendardiv select{padding:0px;}
.org_title{ display: block; font-size: 16px; font-weight: bold; margin: 0; padding: 0; text-align: center;}
.photo_img{background: none repeat scroll 0 0 #F0F0F0; border: 1px solid #CCCCCC; height: 100px;  width: 100px;float:left;}

/*授予角色*/
.roleSelect{margin-left:5px;}
.roleSelect_container em{vertical-align:middle; margin-left:5px;}
.roleSelect_container .roleSelect_ar{padding-top: 10px; border-top:1px solid #99BBE8;}
.roleSelect_ar_role h2{padding: 0 10px 5px;}
.roleSelect_ar_role ul{height:350px; padding:5px; overflow:auto; border-top:1px solid #99BBE8;}
.roleSelect_container .roleSelect_bu{padding: 10px; border-top:1px solid #99BBE8; }
.roleSelect_bu .tab_menu{clear: none;}
.roleSelect_bu .tab_menu ul li{width:60px;}
.roleSelect_bu .container_info{ height:374px; overflow: auto; border: 1px solid #99BBE8;}
/*树的input样式*/
#treeview1 li input{margin-top: -10px;}
/*弹出框*/
.architecture_dialog_basic{width: 600px; z-index:98;top: 100px; margin-left: -300px;}
.architecture_dialog_pe, .architecture_dialog_role{ width: 626px;top: 50px; margin-left: -310px;}
.architecture_dialog, .architecture_dialog_pe{z-index:98;}
.architecture_dialog .architecture_dialog_role{z-index:1001;}
.dialog_content .ml5{margin-left:5px;}
.dialog_content1{background-color: #FFFFFF; border-left: 1px solid #CCCCCC; min-height: 700px; overflow: hidden; width: auto;}
.dialog_content1 .role_info span, .dialog_content1 .role_title span { display: inline-block; text-align: center;  vertical-align: middle; width: 33.3%;margin-left: 0px; padding: 0px;}
.dialog_content1 .bn{border-top:none; border-right:none;}
.architecture_org{border:1px solid #ccc; margin:10px;}
.d_em{display: inline-block; margin-left: 10px;}
.dialog .but{padding-top: 10px;text-align: center;}
.dialog .but input{margin:0 3px;}
.dialog_show{cursor: pointer;}
.role_black{background: none repeat scroll 0 0 rgba(0, 0, 0, 0.5); height: 100%; left: 0; opacity: 0.5; position: fixed; top: 0; width: 100%;display: none; z-index: 1000;}

.obj_input{width:80%;}
.redStar{color:red}

.staff_menu{margin:10px 0 0 5px;}
.staff_content{border-top:1px solid #99BBE8; padding:10px 10px 0 10px;}
.staff_info li{padding:2px 0;}
.staff_info label {text-align: right; width: 100px;float: left; line-height: 26px;}
.staff_info span {line-height: 26px; height: 26px;}
.staff_info input[type="text"], .staff_info input[type="password"]{width: 200px;}
.staff_li{display:none;}

#serviceAddStaff_Dialog .staffInfo_value{display:none;}
#serviceStaff_Dialog .staffInfo_input, #serviceStaff_Dialog .staffInfo_save{display:none;}
.personnel_dialog{top: 200px; width: 400px; z-index: 100; margin-left: -200px;}
.personnel_info{border-bottom: 1px solid #99BBE8; font-size: 14px; padding: 20px 0; text-align: center;}
.personnel_info span{font-weight:bold; color:red;}
.detect{ -moz-transition: -moz-box-shadow 0.2s linear 0s; background: -moz-linear-gradient(center top , #FFFFFF, #DDDDDD) repeat scroll 0 0 transparent; border: 1px solid #999999; border-radius: 5px 5px 5px 5px; box-shadow: 0 1px 0 rgba(255, 255, 255, 0.7), 0 -1px 0 rgba(0, 0, 0, 0.09); color: #333333; cursor: pointer; display: inline-block; letter-spacing: 2px;line-height: 1; margin-left: 10px; overflow: visible; padding: 5px 10px; text-align: center; text-shadow: 0 1px 1px #FFFFFF; width: auto;}

.highOrg_tree{position: absolute; width: 398px; max-height: 270px; overflow: auto; border: 1px solid #AAAAAA;background-color: #FFFFFF;}
.dialog_table tr td .highOrg_tree{line-height:14px;}

/*基本信息弹出框*/
.dialog_table tr td {color: #000; padding:5px 3px; line-height: 22px;}
.dialog_table tr td.menu_td {text-align: right;vertical-align: top;width: 80px;}
.dialog_table tr td input{width: 442px;}
