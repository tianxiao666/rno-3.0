/*Deson Edit*/
@charset "utf-8";

.architecture{margin: 5px;border: 1px solid #99BBE8;}
.architecture_menu_title{background: url("../../../images/white-top-bottom.gif") repeat-x scroll 0 -1px transparent;border-bottom: 1px solid #99BBE8; color: #15428B; font-weight: bold;line-height: 26px; padding: 0 5px; text-align:center;}
.architecture_structure_menu {width: 245px;padding: 10px 0 0 5px;height: 26px;border-bottom: 1px solid #99BBE8;}
.architecture_structure_menu li{cursor: pointer; float:left;padding: 5px; margin:0 5px; width: 75px;background-color: #F0F0F0; border: 1px solid #99BBE8; border-bottom:none; text-align:center;border-radius: 3px 3px 0 0;}
.architecture_structure_menu .selected{background-color: #fff; border-bottom: 1px solid #FFFFFF; margin-bottom: -1px;}
.architecture_left{width: 250px; float:left;}
.architecture_left .architecture_tree{border-right: 1px solid #99BBE8;height: 690px; overflow: auto;}
.img_right{float: left; height: 49px;width: 14px;margin:350px 5px 0 -1px;cursor: pointer; }
.architecture_right{width: auto; overflow: hidden; height: 690px;}
.architecture_right_title{font-size: 16px;font-weight: bold;padding-bottom: 5px;text-align: center;}
.container_info{padding: 10px; border: 1px solid #99BBE8; border-right:none;}
.container_info_in .ml100{margin-left:100px;}
.container_info_in .but{padding: 10px;text-align: center;}
.container_info_in_table .menu_td { text-align: right;vertical-align: top;width:5%;}
.container_info_in_table tr td { color: #000000; line-height: 25px; padding: 6px; vertical-align: middle; white-space: nowrap;}
.container_info_in_table .w_365{width:390px;}
.container_info_in_table .w_160{width:160px;}
.container_info_in_table .w_120{width:120px;}
.container_info_in_table .w_95{width:95px;}
.container_info_pe .page{float: right;padding: 5px 0;}
.container_info_pe_top{padding: 10px 0;}
.container_info_pe_top .pe_left{float: left; }
.container_info_pe_top .pe_right{float: right;}
.show_hide{float: left; height: 49px;width: 14px;margin:350px 5px 0 -1px;cursor: pointer; }
.tab_menu { clear: both;height: 25px;}
.tab_menu ul {position: absolute; z-index: 2;}
.tab_menu ul li { background: none repeat scroll 0 0 #EEEEEE;border-right: 1px solid #99BBE8;border-top: 1px solid #99BBE8;cursor: pointer; float: left;line-height: 24px; padding: 0 3px; position: relative; text-align: center; width: 100px;}
.tab_menu ul li.selected {background: none repeat scroll 0 0 #FFFFFF; border-right: 1px solid #99BBE8;border-top: 3px solid #99BCE8; height: 23px; line-height: 22px;}
