/* CSS Document */
/* */
#header{text-align:center;width:944px; margin:0 auto;}
#container{width:960px; margin:0 auto;}
.header-top{ background:url(../images/white-top-bottom.gif) repeat-x;line-height:35px; color:#15428B; font-size:14px; border-bottom:#99BBE8;border-left: 1px solid #99BBE8;border-right: 1px solid #99BBE8;}
.header-main{ line-height:20px; border:1px solid #99BBE8; background:#DFE8F6; padding:3px 5px;}

.container-main{ margin-top:10px;}
.container-tab1 fieldset{ border:1px solid #B5B8C8; color:#15428B; padding:10px; width:96%; margin:5px auto;}
.container-tab1 fieldset .container-tab1-message{ margin-left:20px;}
.container-tab1 fieldset .container-tab1-message span{ font-weight:normal; color:#666;}
.container-bottom { text-align:center; padding:0; padding-bottom:20px;}
.fieldset-hide{padding-bottom:0 !important; border-width: 1px 0 0 0 !important;}/* fieldset隐藏选用样式 */

.container-tab2{ margin: 0 auto;min-width:700px; clear:both;border-bottom: 1px solid #CCCCCC; margin-bottom:10px;}
.container-tab2 ul{ width:100%; line-height:25px;clear:both;}
.container-tab2 ul li{ float:left; padding:0 5px; border:1px solid #ccc; background:#DFE8F6; border-left:none;min-width:90px; text-align:center; border-bottom:none;}
.container-tab2 ul li a{ display:block; color:#333; font-size:12px; font-weight:bold; text-decoration:none;}
.container-tab2 ul .tab2-li-show{background: #FFFFFF;}/* tab选用样式 */
/* 表格样式1，通用 */
.container-main-table1{margin: 0 auto; width:100%; position:relative;}
.main-table1{ width:100%; margin: 0px auto 5px; background:#FFF; position:relative;}
/* 鼠标经过table样式变化 */
.mouseover-table{ background-color:#000;}
.main-table1 th{ padding:0px 10px; background:url(../images/cmp-bg2.gif) repeat-x; color:#444; white-space:nowrap; border:#CCC 1px solid; line-height:30px;}
.main-table1 tr td{ border:1px solid #CCC; padding:6px; padding-left:6px; color:#000; line-height:21px; vertical-align:middle; white-space:nowrap; background:WhiteSmoke;}
.main-table2 tr td{ text-align:center;}
.main-table1 tr td a{ color:#03F;}
.main-table1 .main-table1-title{ padding:0px 10px; background:url(../images/cmp-bg2.gif) repeat-x; color:#444; height:30px; line-height:30px;}
.main-table1 .menuTd{ width:10%; background: #E8EDFF;text-align:right; white-space:nowrap;}
.main-table1 .table-head{ text-align:center; border:#000; color:999; font-weight:bold;}
/*.main-table1 .menuTd:hover{ background:#D0DAFD url(../image/gradhover.png) repeat-x;}*/
.main-table1 img{ position:relative; left:-4px; top:3px;}
.main-table1 .higherLine{height:80px;}
.main-table1 tr td select{ width:120px;}
.main-table1 tr td select option{ padding:1px 0;}
.container-main-table1-left{border-top: 1px solid #CCCCCC; float: left;height: auto; margin-left: 2px; margin-right: 2px; min-height: 300px; width: 175px}
.container-main-table1-right{ width:auto; overflow:hidden;}
.container-tab2-right{padding-right:25px;position: relative;text-align: right; top: -37px;margin-left: 210px;}
.checkbox-up{ position:relative; left:-1px; top:-2px;}
#shoucangjia{display:inline;background:url(../images/favorites_folder.png) no-repeat 0px 2px;padding:5px 0 5px 23px;}
#shoucangjia a{ color:#03F;}
#search-table{position:absolute; left:50%;margin-top:-276px; margin-left:-340px; z-index:100; display:none;}
#black{background:rgba(0,0,0,0.5); z-index:15; display:none; width:100%; height:1000px; position:absolute; left:0px; top:0px;}
#people-information{ position:absolute; left:0px; top:0px; display:none; background:#FFF; border:2px solid #999; width:200px; padding:3px;}
.people{ display:block; padding:3px;}
