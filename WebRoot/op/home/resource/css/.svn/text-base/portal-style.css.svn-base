/*-------------工作空间左边导航nav----------------begin--*/


#topNavWrapper {
	width:100%;	
	text-align: left;
	height: 30px;
	margin: 0px;
	padding: 0px;
	margin: 0px auto;
	z-index:100;
	_position: relative ;
	_top:0px;
}
#topNav {
	margin: 0px;
	padding: 0px;
	width:100%;
	float: left;
	display: block;
	z-index: 100;
	overflow: visible;
	position: fixed;
	top: 0px; /* position fixed for IE6 */
	_position: absolute;
	_top: expression(documentElement.scrollTop + "px");
	height: 30px;
}


#nav_div {
width:100%;
/*background: url(../images/nav1.gif) repeat-x ;*/
background: url(../images/white-top-bottom.gif) repeat-x ;
font-family:"微软雅黑"; font-size:12px;
/*
position:fixed;
_position:absolute;_top:expression_r(eval_r(document.documentElement.scrollTop));
z-index:9999999999;
left:0;
top:0;
*/
}
#nav { display: block; margin: 0; padding: 0; position: relative; } 
#nav li { display: block; list-style: none; margin: 0; padding: 0; float: left; position: relative; }
/* #nav a { display: block; font-size:14px;color:#fff;}*/
#nav ul { display: none; position: absolute; left: 0; margin: 0; padding: 0; }
* html #nav ul { line-height: 0; } /* IE6 "fix" */
#nav ul a { zoom: 1; } /* IE6/7 fix */
#nav ul li { float: none; }
#nav ul ul { top: 0; }    
/* Essentials - configure this */
#nav ul { width: 130px; }
#nav ul ul { left: 131px;}
/* Everything else is theming */
 
 .liSelected{
 background: url(../images/nav2.gif) repeat-x; 
 font-weight: bold;
 color: #23508E;
 }
 
#nav {height: 30px; float:left;}
#nav *:hover { background-color: none; }
#nav a { border-right: 1px solid white; font-size:14px;color:#23508E; display: block; padding: 6px 15px 10px 5px; line-height: 1; }
#nav li.hover a { background: url(../images/nav2.gif) repeat-x; }

#nav ul { top: 25px; }
#nav ul li a { background: url(../images/nav2.gif) repeat-x; }
#nav ul a.hover { background: url(../images/nav2.gif) repeat-x; }
#nav ul a { border-bottom: 0.5px solid white; border-right: none; opacity: 2; filter: alpha(opacity=90); }
/* #nav ul a { border-bottom: none; } - I also needed this for IE6/7 */


#nav a:link{text-decoration:none ; color:#23508E ;}
#nav a:visited {text-decoration:none ; color:#23508E;}
#nav a:hover {text-decoration:none ; color:white;}
#nav a:active {text-decoration:none ; color:#23508E;}


/*-------------工作空间左边导航----------------end--*/

	
/*-------------工作空间导航条右边nav2用户信息及相关按钮链接----------------begin--*/
#nav2 {height:30px;line-height: 30px;float:right;margin-right:20px;position: relative;z-index:99999;font-size:12px; font-family:"微软雅黑";}
#nav2 a:link{text-decoration:none ; color:#23508E ;font-weight:bold;}
#nav2 a:visited {text-decoration:none ; color:black;font-weight:bold;}
#nav2 a:hover {text-decoration:none ; color:#000;}
#nav2 a:active {text-decoration:none ; color:#23508E;font-weight: bold;}

.navButton {
	-moz-box-shadow:inset 0px 0px 0px 0px #ffffff;
	-webkit-box-shadow:inset 0px opx 0px 0px #ffffff;
	box-shadow:inset 0px 0px 0px 0px #ffffff;
	background-color:#cccccc;
	-moz-border-radius:5px;
	-webkit-border-radius:5px;
	border-radius:5px;
	border:1px solid #AAAAAA;
	display:inline-block;
	color:#777777;
	FONT-FAMILY: 微软雅黑; 
	font-size:12px;
	font-weight:bold;
	padding:1px 1px;
	text-decoration:none;
	line-height: 20px;
	/*text-shadow:1px 1px 0px #ffffff;*/
}.navButton:hover {
	background-color:#dfdfdf;
}.navButton:active {
	position:relative;
	top:1px;
}

