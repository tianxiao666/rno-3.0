body {color: #000000; font-size: 12px;line-height: 166.6%;text-align: center;}
html,body,div,dl,dt,dd,span,a{margin:0; padding:0;}
body, input, select, button {font-family: verdana;}
h1, h2, h3, select, input, button {font-size: 100%;}
body, h1, h2, h3, ul, li, form, p, img { border: 0 none; margin: 0; padding: 0;}
input, button, select, img {line-height: normal; margin: 0;}
select {padding: 1px;}
ul {list-style: none outside none;}
select, input, button, button img, label {vertical-align: middle;}
header, footer, section, aside, nav, hgroup, figure, figcaption {border: medium none; display: block; margin: 0; padding: 0;}
em{font-style:normal;}
a {color: #848585;text-decoration: none;}
a:hover {color: #33A3DC; text-decoration: underline;}
.clearfix:before, .clearfix:after { slide: " ";display: block; height: 0;overflow: hidden;}
.clearfix:after {clear: both;}
.login_main {background-color:#FFFFFF; height: 500px; margin-top: 50px;}
.login_inner {width: 960px; margin: 0 auto; position: relative;}
.login_logo{background: url("../images/iosm_logo.png") no-repeat; height: 60px;position: absolute; left: 0; width: 160px; margin-top:20px;}
.login_header {background: url("../images/login_header.png") no-repeat; height: 100px;position: absolute; width: 400px; left:150px;}
.version_number{background: url("../images/version.png") no-repeat; width:80px; height:48px;left: 520px; position: absolute; top: -20px;}
.version_number em{display: inline-block; font-style: normal; line-height: 54px; color:#333; margin-left:5px;}
