.login_slide {padding-top:100px;}
.login_slide .slides > li {display: none;} 
.login_slide .slides img {max-width: 100%; display: block;}
.login_slide .slides {zoom: 1;}
.login_slide .slides > li {position: relative;}
.login_slideNav {position: absolute; bottom: -390px; *+bottom: 25px; left:10px; text-align: center;}
.login_slideNav li {margin: 0 0 0 5px; display: inline-block; zoom: 1; *display: inline;}
.login_slideNav li:first-child {margin: 0;}
.login_slideNav li a {width: 13px; height: 13px; display: block; background: url("../images/bg_control_nav.png") no-repeat 0 0; cursor: pointer; text-indent: -9999px;}
.login_slideNav li a:hover {background-position: 0 -13px;}
.login_slideNav li a.active {background-position: 0 -26px; cursor: default;}