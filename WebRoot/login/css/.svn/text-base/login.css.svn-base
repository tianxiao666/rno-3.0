.login_block {border: 1px solid #B7C2C9; border-radius: 2px 2px 2px 2px; height: 350px; margin-top: 25px; *+margin-top: 125px; overflow: hidden; position: absolute; right: 30px; text-align: left; width: 300px; z-index: 2; *+top:0px; box-shadow: 2px 5px 5px 0 rgba(0, 0, 0, 0.5);}
.login_title {background-image: url("../images/login_block.png"); background-position: 0 0; background-repeat: repeat-x; border-right: 1px solid #B7C2C9; color: #1c70c5; cursor: pointer; float: left; font-family: "Microsoft YaHei" ! important; font-size: 18px; font-weight: 700; height: 49px; line-height: 46px; overflow: hidden; position: relative; text-align: center; width: 300px;}
.login_formbg {background-color: #FFFFFF; height: 301px; margin-top: 49px; position: relative; opacity: 0.8; filter:alpha(opacity=80)!important;}
.login_form {height: 290px; position: absolute; top: 60px; left: 30px;}
.login_formIpt {clear: both; height: 34px; margin-top: 25px; position: relative; width: 238px; }
.loginFormTdIpt {border:solid 1px #ccc;box-shadow:0 1px 10px rgba(0,0,0,0.1) inset; color: #999; font-family: "Î¢ÈíÑÅºÚ"; font-size: 16px; height: 23px;ime-mode: disabled; left: 0; padding: 5px 8px; top: 0;width: 220px;}
.loginFormTdIpt:focus{box-shadow:0 0 4px rgba(28,112,197,0.8);border:solid 1px #1C70C5;}
#passWord{display:none;}
.login_form .verificationCode{width:116px;}
.login_form .verificationCode .loginFormTdIpt{width:120px;}
.login_form .verificationCode .img{top: 0;left: 148px; position: absolute;}
.login_form .verificationCode .img img{height:35px; width:90px;}
.loginFormCheck {color: #000; line-height: 14px; margin: 25px 0 0 42px;}
.loginFormIptWiotTh {border: medium none; height: 35px;margin: 30px 0 0 60px;}
.login_formIpt .userNameIpt{width: 103px;}
.login_formIpt .email_text{background-color: #f0f0f0; border: 1px solid #CCCCCC; border-left:none; overflow: hidden; cursor: pointer; display: block; font-size: 12px; height: 33px; line-height: 33px; padding: 0 3px; position: absolute; top: 0px; *+top:1px; left: 121px; width: 110px;}
.login_formIpt .email_text em{display:inline-block;}
.login_formIpt .email_text em.email_name{width:95px; white-space:nowrap;overflow:hidden;text-overflow:ellipsis;}
.login_formIpt .email_text em.em_arrow{position: absolute; right: 3px; top: 14px; color: #999; font-size:10px;border-left: 5px solid transparent; border-right: 5px solid transparent; border-top: 5px solid #999;}
.login_formIpt .email_list{display:none; background-color: #f0f0f0; border: 1px solid #CCCCCC; border-top:none; position: absolute; left:120px; *+top:36px; z-index: 100; width:116px; }
.login_formIpt .email_list ul li{font-size:12px; height: 26px; line-height:26px; padding:0 5px; cursor:pointer; white-space:nowrap;overflow:hidden;text-overflow:ellipsis;}
.login_formIpt .email_list ul li.hover{background-color:#5399e0;}
.login_form em.errorTip{color: red;display: inline-block; position: absolute;text-align: left;top: 0; width: 240px;}
.login_form em.errorTipVc{color: red;display: inline-block;}
.btn {background-image: url("../images/login_block.png");border: 0 none; cursor: pointer; display: inline-block; font-family: "Microsoft YaHei" ! important; font-size: 18px;font-weight: 700;height: 35px;line-height: 35px;outline: 0 none;padding: 0;text-align: center; vertical-align: baseline;}
.btn_login {background-position: -2px -51px;color: #FFFFFF;width: 102px;}
.btn_login:hover {background-position: -116px -51px;}
.login_footer {background-color:#FFFFFF; height: 65px; margin: 0 auto;}
.footer_inner {color: #848585; height: 63px; margin: 0 auto; overflow: visible; position: relative; width: 960px;}
.footer_logo {left: 35px;top: 11px;position:absolute;}
.footer_nav {top: 25px;position:absolute;right: 100px;}
.footer_nav a {margin:0 15px;}
.footer_nav .copyright{margin-left:15px}
.footer_kx{position:absolute;right:30px;top:24px;}
