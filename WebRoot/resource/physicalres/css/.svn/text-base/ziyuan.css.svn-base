/* CSS Document */
body{font: 12px/1.5 Tahoma,'Microsoft Yahei','Simsun';}
#up,#down{min-width:974px; width:98%;margin:0 auto;}
#down{margin-top:10px; background:#f9f9f9; border:1px solid #99BCE8; }
#down2{ border:1px solid #99BCE8; border-top:none;}


#up_top{ width:100%-2px; line-height:23px; background:url(../image/white-top-bottom.gif) repeat-x; border:1px solid #99BCE8; padding-left:5px;}
#up_down{ width:100%-2px; border:1px solid #99bce8; height:180px;background:#f9f9f9;}

#down_top{ padding:5px 10px;line-height:25px;}
#down_top span h4{color:#15428B;}
/*#down_left{ display:inline-block; width:53%; border-right:1px solid #ccc; height:180px; background:#f9f9f9;}*/
#down_left{ display:inline-block; width:53%;height:180px;}
#down_left table{ width:80%; margin:10px auto;}
#down_right{ display:inline-block; width:43%; float:right;}
/*#up table{ width:94%; text-align:left;}*/
#up table{text-align:left;position:relative;}
#up table th img{ position:relative; top:3px; right:-3px; cursor:pointer;}
#up table td{padding:4px 3px; white-space:nowrap;}
#up table .menuTd{line-height:23px; text-align:right;}

fieldset{border: 1px solid #CCCCCC;padding: 0 0px;}
#down_right fieldset{
    height: 175px;
    width: 413px;}
#down_left fieldset {
    height: 175px;

}
#down table {width:98%; margin:10px auto;}
#down table td{border:1px solid #ccc;}
#down table th{ border:1px solid #ccc; background:url(../image/cmp-bg.gif) repeat-x; line-height:21px;}

	
/*弹出框*/
#body_div table{ width:99%; margin:3px auto; }
#body_div table td{ padding:3px 5px;}
.body_div_top{ width:100%-2px; line-height:23px; background:url(../image/white-top-bottom.gif) repeat-x; border:1px solid #99BCE8; padding-left:5px;}
.body_div_bottom{ text-align:left;margin-bottom:10px;margin: 6px 340px;}

