/*字典查询*/
.filter_dialog_but{background-color: #4c86d6; border: 1px solid #2B4F82; border-radius: 3px 3px 3px 3px; color: #FFFFFF; cursor: pointer; display: inline-block; font-weight: bold; }
.filter_dialog_but:hover{background-color: #334bdc;}
.filter_dialog_but .but_text{ font-size: 14px; line-height: 24px; padding: 0 5px 0 10px;}
.filter_dialog_but .but_icon{border-color: transparent #0e2976 #0e2976 transparent;border-image: none;border-style: dashed solid solid dashed;border-width: 10px;display: inline-block;height: 0;width: 0;vertical-align: middle;line-height: 0; margin-top:5px;}
.filter_dialog{background-color: #FFFFFF; border: 3px solid #99BCE8; display: none; position: absolute; width: 400px; z-index: 1000; margin-top:2px;}
.filter_dialog h2{font-weight: normal; cursor: pointer; }
.filter_dialog .filter_dialog_info{ color: #333333; font-weight: normal; height: 26px; line-height: 26px;}
.filter_dialog .filter_main{cursor: pointer; display: none; top: 0px; position: absolute;}
.filter_dialog .filter_main li{float: left; height: 26px;line-height: 26px; letter-spacing: 0; list-style: none outside none; overflow: hidden; padding-left: 10px;}
.filter_dialog .filter_main a:hover{text-decoration:underline; color:#000; font-weight:bold;}

.filter_specialty_info{*+margin:30px 0 0 -78px;}
.filter_specialty_info .filter_dialog_info{ width: 100px;border-right: 1px solid #99BCE8;border-top: 1px solid #99BCE8;}
.filter_specialty_info .filter_dialog_info:first-child{border-top:none;}
.filter_specialty_info h2{background-color: #d1e2f6; padding-left:10px;}
.filter_specialty_info .filter_active{ z-index: 99;border-right: 1px solid #fff;}
.filter_specialty_info .filter_active h2{font-weight: bold; background-color:#fff;}
.filter_specialty_info .filter_main{left: 100px;width: 300px;}
.filter_specialty_info .filter_main li{width: 65px;}

.filter_letter_info{*+margin:30px 0 0 -78px;}
.filter_letter_info .filter_dialog_info{ border: 1px solid #6593cd; float: left; margin: 5px; width: 26px;}
.filter_letter_info h2{background-color: #88b9f5;text-align: center;}
.filter_letter_info .filter_active h2{font-weight: bold; background-color:#fff;}
.filter_letter_info{height: 380px;width: 353px;}
.filter_letter_info .filter_letter_info_left{background-color: #d1e2f6; width: 114px; border-right: 1px solid #99BCE8;}
.filter_letter_info .filter_main{left: 109px;width: 240px; height:530px; overflow:auto;}
.filter_letter_info .filter_main li{width: 70px;}

.filter_dictionary_info{background-color: #FFFFFF; border: 3px solid #99BCE8; display: none; position: absolute; width: 200px; z-index: 1000;margin-top:2px;*+margin:30px 0 0 -118px;}
.filter_dictionary_info ul{height:500px; overflow:auto;}
.filter_dictionary_info ul li{line-height:26px; padding-left:10px; border-top: 1px solid #c3d8f2;}
.filter_dictionary_info ul li:first-child{border-top:none;}
.filter_dictionary_info ul li:hover{background-color: #c3d8f2; font-weight:bold; text-decoration:underline;cursor: pointer;}

/*查询结果列表*/
.aetg{background-color: #CDDFF5; max-height: 327px; overflow: auto; position: absolute; top: 55px; width: 100%;}
.aetg ul li{line-height:24px; padding: 0 5px; cursor:pointer;}
.aetg ul li:hover{background-color: #88B9F5; }
.aetg .aetg_text{display: inline-block; width: 160px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.aetg .aetg_link{position:absolute; right:0px; display:inline-block;}
.aetg a{padding:0 3px;}

.aetg1{background-color: #CDDFF5; max-height: 327px; overflow: auto; position: absolute; top: 25px; width: 100%;}
.aetg1 ul li{line-height:24px; padding: 0 5px; cursor:pointer;}
.aetg1 ul li:hover{background-color: #88B9F5; }
.aetg1 .aetg_text{display: inline-block; width: 160px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.aetg1 .aetg_link{position:absolute; right:0px; display:inline-block;}
.aetg1 a{padding:0 3px;}
/*弹出框*/
.dialog_box{width: 530px; background: url(../image/panel_title.png) repeat scroll 0 0 transparent; border: 1px solid #99BBE8; border-radius: 5px 5px 5px 5px; font-size: 12px; top: 100px; padding: 5px; left: 45%; margin-left: -200px; position: absolute;z-index:10000;}
.dialog_header{ color: #15428B; font-size: 12px; font-weight: bold; line-height: 15px; padding: 2px 0 4px; position: relative;}
.dialog_tool { position: absolute; right: 1px; top: 0px;}
.dialog_tool div {cursor: pointer; display: block;float: right; height: 16px; margin-left: 2px; opacity: 0.6; width: 16px;}
.dialog_tool_close {background: url(../image/panel_tools.gif) no-repeat scroll -16px 0 transparent;}
.dialog_content{border: 1px solid #99BBE8; background-color:#fff;padding:5px;}
.dialog_content ul li{padding:0 5px;}
.dialog_content label{display: block;float: left;text-align: right;vertical-align: middle;width: 85px;}
.dialog_content span{padding: 0px 5px;text-align: left;vertical-align: middle;margin-left: 85px; color:#444;}
.dialog_black {background: none repeat scroll 0 0 rgba(0, 0, 0, 0.5); height: 100%; left: 0; opacity: 0.5; position: fixed; top: 0; width: 100%;display: none;z-index: 97;}

.dialog_but{text-align: center; white-space: nowrap;}
.dialog_but button { margin:25px 10px 0 10px; padding: 6px 12px; cursor: pointer; display: inline-block; text-align: center; line-height: 1; *padding:4px 10px; *height:2em; letter-spacing:2px; font-family: Tahoma, Arial/9!important; width:auto; overflow:visible; *width:1; color: #333; border: solid 1px #999; border-radius: 5px; background: #DDD; filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#FFFFFF', endColorstr='#DDDDDD'); background: linear-gradient(top, #FFF, #DDD); background: -moz-linear-gradient(top, #FFF, #DDD); background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#FFF), to(#DDD)); text-shadow: 0px 1px 1px rgba(255, 255, 255, 1); box-shadow: 0 1px 0 rgba(255, 255, 255, .7),  0 -1px 0 rgba(0, 0, 0, .09); -moz-transition:-moz-box-shadow linear .2s; -webkit-transition: -webkit-box-shadow linear .2s; transition: box-shadow linear .2s; }
.dialog_but button::-moz-focus-inner{ border:0; padding:0; margin:0; }
.dialog_but button:focus { outline:none 0; border-color:#426DC9; box-shadow:0 0 8px rgba(66, 109, 201, .9); }
.dialog_but button:hover { color:#000; border-color:#666; }
.dialog_but button:active { border-color:#666; filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#DDDDDD', endColorstr='#FFFFFF'); background: linear-gradient(top, #DDD, #FFF); background: -moz-linear-gradient(top, #DDD, #FFF); background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#DDD), to(#FFF)); box-shadow:inset 0 1px 5px rgba(66, 109, 201, .9), inset 0 1px 1em rgba(0, 0, 0, .3); }
.dialog_but button[disabled] { cursor:default; color:#666; background:#DDD; border: solid 1px #999; filter:alpha(opacity=50); opacity:.5; box-shadow:none; }
.dialog_but .aui_state_highlight { color: #FFF; border: solid 1px #1c6a9e; background: #2288cc; filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#33bbee', endColorstr='#2288cc'); background: linear-gradient(top, #33bbee, #2288cc); background: -moz-linear-gradient(top, #33bbee, #2288cc); background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#33bbee), to(#2288cc)); text-shadow: -1px -1px 1px #1c6a9e; }
.dialog_but .aui_state_highlight:hover { color:#FFF; border-color:#0F3A56; }
.dialog_but .aui_state_highlight:active { border-color:#1c6a9e; filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#33bbee', endColorstr='#2288cc'); background: linear-gradient(top, #33bbee, #2288cc); background: -moz-linear-gradient(top, #33bbee, #2288cc); background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#33bbee), to(#2288cc)); }

/*自定义导出弹出框*/
#dialog_export{width: 530px; background: url(../image/panel_title.png) repeat scroll 0 0 transparent; border: 1px solid #99BBE8; border-radius: 5px 5px 5px 5px; font-size: 12px; top: 100px; padding: 5px; left: 45%; margin-left: -200px; position: absolute;z-index:98;}
.dialog-header_export{ color: #15428B; font-size: 12px; font-weight: bold; line-height: 15px; padding: 2px 0 4px; position: relative;}
.dialog-tool_export { position: absolute; right: 1px; top: 0px;}
.dialog-tool_export div {cursor: pointer; display: block;float: right; height: 16px; margin-left: 2px; opacity: 0.6; width: 16px;}
.dialog-tool-close_export {background: url("../image/panel_tools.gif") no-repeat scroll -16px 0 transparent;}
.dialog_content_export{border: 1px solid #99BBE8; background-color:#fff;}
.dialog-content-title_export{line-height:28px; border-bottom:1px solid #ccc; background:#eee;padding-left:6px;}
.dialog_content_export .tb_1{width:100%; border-bottom:1px solid #ccc;}
.dialog_content_export .tb_1 td{width:25%; padding-left:10px;line-height: 25px; padding:5px 10px;}
.dialog_content_export label { width: 70px; display: block; float: left; text-align: right; vertical-align: middle;}
.dialog_content_export span { margin-left: 70px; display: block; padding: 0 5px; text-align: left; vertical-align: middle;}
.dialog_content_export textarea{width:90%}
.d-em_export{display: inline-block; margin-left: 10px;}
.dialog_content_export .but{text-align: center; padding:4px 0px; background:#eee;}
.dialog_content_export .but input{margin:0 5px;}
.dialog-show_export{cursor: pointer;}

/*加入类似注释*/
.wh_tip{display:inline-block; height:16px; width:16px; border:1px solid green; border-radius:10px; text-align:center; background:lightgreen; cursor:pointer; margin-left:4px;}
.wh_img{position:absolute;display:inline-block; border:1px solid #ccc; z-index:5;right:-364px;margin-top:32px;padding:2px;border-radius:5px;background:yellow;}
.tip_sj{display:inline-block; position:absolute; top:-10px; right:50%;border-bottom: 10px solid yellow; border-left: 10px solid transparent;border-right: 10px solid transparent;}

/*面板切换 yuan.yw add*/
.tab_2_ul_panel{clear:both;}
.tab_2_ul_panel li{float:left; border-right:1px solid #ccc;text-align:center; min-width:60px; padding:0px 5px; cursor:pointer; background:#f6f6f6; line-height:28px; position:relative;}
.tab_2_ul_panel li.ontab{background:#fff;z-index:2; font-weight:bold;}
.tab_2_ul_viewpanel{clear:both;}
.tab_2_ul_viewpanel li{float:left; border:1px solid #ccc;text-align:center; width:160px; margin:0 0 4px 4px; padding:0px 5px; cursor:pointer; background:#f6f6f6; line-height:28px; position:relative;}
.tab_2_ul_viewpanel li.ontab{background:#fff;z-index:2; font-weight:bold;}

/*错误-警告提示*/
.tooltip_icon{float: left; margin:0 20px 0 10px;}
.tooltip_error{background:url(../image/error.png) no-repeat; height:48px; width:48px;}
.tooltip_warn{background:url(../image/warn.png) no-repeat; height:48px; width:48px;}
.tooltip_icon p{text-align:center;font-size: 14px;}
.tooltip_info{overflow: hidden;}
.tooltip_info p{line-height:24px;color:red;}
.tooltip_info p em{color:blue;}

/*dacheng add loading*/
.insp_black {background-color: #fff; display: none; height: 100%; left: 0; opacity: 0.5; position: fixed; top:50%; width: 100%; z-index: 10; text-align: center;padding-top:0px;}
.bottom_load{display: none; padding:150px 0;}
