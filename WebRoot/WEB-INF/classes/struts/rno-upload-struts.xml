			<result name="success">/op/rno/rno_res_eri_fas.jsp</result>
	</action>
	<!-- 询问此时是否可以上传文件  -->
	<action name="ifAllowedUploadFileAjaxAction" class="rnoFileUploadAction"
			method="ifAllowedUploadFileAjaxAction">
	</action>
	<!-- 询问是否接收待上传的文件 -->
	<action name="ifFileAcceptableAjaxAction" class="rnoFileUploadAction"
			method="ifFileAcceptableAjaxAction">
	</action>
	<!-- 查询上传进度 -->
	<action name="queryUploadFileProgressAction" class="rnoFileUploadAction"
			method="queryUploadFileProgressAction">
	</action>
	<!-- 离线上传 -->
	<action name="batchUploadFileAjaxAction" class="rnoFileUploadAction"
			method="batchUploadFileAjaxAction">
	</action>
	<!-- 查询上传的数据记录 -->
	<action name="queryUploadRecAjaxAction" class="rnoFileUploadAction"
			method="queryUploadRecAjaxAction">
	</action>
	
	<!-- 查询报告 -->
	<action name="queryJobReportAjaxAction" class="rnoFileUploadAction" method="queryJobReportAjaxAction"></action>
     
