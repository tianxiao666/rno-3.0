			<result name="success">/op/rno/rno_res_2g_mrr.jsp</result>
		</action>
		<!-- 初始化用户ncs数据管理页面 -->
	 <action name="initRno2GNcsManageAction" class="rnoResourceManagerAction"
			method="initRno2GNcsManageAction">
			<result name="success">/op/rno/rno_res_2g_ncs.jsp</result>
		</action>
	<!-- 初始化用户爱立信ncs数据管理页面 -->
	 <action name="initRno2GEriNcsManageAction" class="rnoResourceManagerAction"
			method="initRno2GEriNcsManageAction">
			<result name="success">/op/rno/rno_res_2g_eri_ncs.jsp</result>
