/* 2013.09.17增加搜索结果表格*/
.search_result_standardtable_gray {    border: none;    width: 100%;}
.search_result_standardtable_gray th {
    background: url("../images/cmp-bg2.gif") repeat-x scroll 0 0 transparent;
    border-bottom: 1px solid #CCCCCC;
    border-right: 1px solid #CCCCCC;
    height: 30px;
    line-height: 22px;
    text-align: center;
	font-weight: bold;
    white-space: nowrap;
	
}
.search_result_standardtable_gray td {
    /*border-bottom: 1px solid #EEEEEE; */
	border:1px solid #EEEEEE;
	height: 24px;
    line-height: 24px;
    overflow: hidden;
    padding: 2px 4px;
    /*text-align: left; */
    white-space: nowrap;
}

.search_result_standardtable_gray	.search_result_standardtable_gray_sumtitle td{
	border:1px solid #cccccc;
	background-color: #f4f7fc;
	/*text-align: center;*/
	height:28px;
	border-bottom: 1px solid #CCCCCC;

}
.search_result_standardtable_gray .search_result_standardtable_gray_value_sytle{ text-align:right}
.search_result_standardtable_gray .search_result_standardtable_gray_date_sytle{ text-align:center}
