/* CSS Document */
.pagingDiv{line-height:30px;border-top:1px solid #ccc;text-align:right;background:#f6f6f6;}
.paging_div{ font-family:"΢微软雅黑";padding-right:5px;line-height:30px;border-top:1px solid #ccc;text-align:right;background:#f6f6f6;}
input.paging_input_text{height:18px;vertical-align:middle; line-height:15px; padding:3px 1px 2px 1px; width:32px; text-align:center; background:#fff;}
select.paging_input_text{padding:1px;width:45px;background:#fff;vertical-align:middle;}
a.paging_link{cursor:pointer;height:19px; line-height:18px; width:20px; margin-left:3px; text-align:center; background:#fff; border:1px solid #ccc; color:#000; text-decoration:none;display:inline-block; vertical-align:middle; border-radius:2px;}
a.page-first{ background:url(images/page-first.gif) 2px 2px no-repeat;}
a.page-prev{ background:url(images/page-prev.gif) 2px 2px no-repeat;}
a.page-next{ background:url(images/page-next.gif) 2px 2px no-repeat;}
a.page-last{ background:url(images/page-last.gif) 2px 2px no-repeat;}
a.page-go{padding:0px 2px;color:#009;}
a.paging_link:hover{border:1px solid #99BBE8; box-shadow:1px 1px 3px #bbb;}
.paging_text{ vertical-align:middle; font-size:12px;}