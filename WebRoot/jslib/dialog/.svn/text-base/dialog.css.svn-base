/*Deson Edit*/
@charset "utf-8";

/*弹出框样式*/
.dialog{background: url("images/panel_title.png") repeat scroll 0 0 transparent; border: 1px solid #99BBE8; border-radius: 5px 5px 5px 5px; font-size: 12px; padding: 5px; position: absolute; z-index:20; left:50%;}
.dialog2{background: url("images/panel_title.png") repeat scroll 0 0 transparent; border: 1px solid #99BBE8; border-radius: 5px 5px 5px 5px; font-size: 12px; padding: 5px; position: absolute; z-index:20; /*left:50%;*/}
.dialog_header{ color: #15428B; font-size: 12px; font-weight: bold; line-height: 15px; padding: 2px 0 4px; position: relative;}
.dialog_tool { position: absolute; right: 1px; top: 0px;}
.dialog_tool div {cursor: pointer; display: block;float: right; height: 16px; margin-left: 2px; opacity: 0.8; width: 16px;}
.dialog_tool_close {background: url("images/panel_tools.gif") no-repeat scroll -16px 0 transparent;}
.dialog_content{border: 1px solid #99BBE8; background-color:#fff;padding:10px 0;}
.dialog_content_grey{border: 1px solid #99BBE8; background-color:#fffdf9;padding:10px 0;}
.dialog_but{text-align: center; white-space: nowrap;}
.dialog_but button { margin:10px 10px 0 10px; padding: 6px 12px; cursor: pointer; display: inline-block; text-align: center; line-height: 1; *padding:4px 10px; *height:2em; letter-spacing:2px; font-family: Tahoma, Arial/9!important; width:auto; overflow:visible; *width:1; color: #333; border: solid 1px #999; border-radius: 5px; background: #DDD; filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#FFFFFF', endColorstr='#DDDDDD'); background: linear-gradient(top, #FFF, #DDD); background: -moz-linear-gradient(top, #FFF, #DDD); background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#FFF), to(#DDD)); text-shadow: 0px 1px 1px rgba(255, 255, 255, 1); box-shadow: 0 1px 0 rgba(255, 255, 255, .7),  0 -1px 0 rgba(0, 0, 0, .09); -moz-transition:-moz-box-shadow linear .2s; -webkit-transition: -webkit-box-shadow linear .2s; transition: box-shadow linear .2s; }
.dialog_but button::-moz-focus-inner{ border:0; padding:0; margin:0; }
.dialog_but button:focus { outline:none 0; border-color:#426DC9; box-shadow:0 0 8px rgba(66, 109, 201, .9); }
.dialog_but button:hover { color:#000; border-color:#666; }
.dialog_but button:active { border-color:#666; filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#DDDDDD', endColorstr='#FFFFFF'); background: linear-gradient(top, #DDD, #FFF); background: -moz-linear-gradient(top, #DDD, #FFF); background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#DDD), to(#FFF)); box-shadow:inset 0 1px 5px rgba(66, 109, 201, .9), inset 0 1px 1em rgba(0, 0, 0, .3); }
.dialog_but button[disabled] { cursor:default; color:#666; background:#DDD; border: solid 1px #999; filter:alpha(opacity=50); opacity:.5; box-shadow:none; }
.dialog_but .aui_state_highlight { color: #FFF; border: solid 1px #1c6a9e; background: #2288cc; filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#33bbee', endColorstr='#2288cc'); background: linear-gradient(top, #33bbee, #2288cc); background: -moz-linear-gradient(top, #33bbee, #2288cc); background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#33bbee), to(#2288cc)); text-shadow: -1px -1px 1px #1c6a9e; }
.dialog_but .aui_state_highlight:hover { color:#FFF; border-color:#0F3A56; }
.dialog_but .aui_state_highlight:active { border-color:#1c6a9e; filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#33bbee', endColorstr='#2288cc'); background: linear-gradient(top, #33bbee, #2288cc); background: -moz-linear-gradient(top, #33bbee, #2288cc); background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#33bbee), to(#2288cc)); }
.black{background-color: #000; height: 100%; left: 0; opacity: 0.5; position: fixed; top: 0; width: 100%;display: none; z-index:10; filter:alpha(opacity=50)!important; }

