<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.iscreate.plat.tree.TreeNode" table="TREENODE_OPS" >
        <id name="id">
            <column name="id" />
            <generator class="sequence">
            	<param name="sequence">
            		SEQ_TREENODE_OPS
            	</param>
            </generator>
        </id>
        <property name="treeId" column="treeId" />
        <property name="treeNodeName" column="treeNodeName" />
        <property name="treeNodeDesc" column="treeNodeDesc" />
        <property name="parentId" column="parentId" />
        <property name="referenceType" column="referenceType" />
        <property name="referenceValue" column="referenceValue" />
        <property name="updateDate" column="updateDate" />  
        <property name="status" column="status" />
    </class>
</hibernate-mapping>
