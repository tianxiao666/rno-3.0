package com.iscreate.plat.workflow.datainput;

public class NotionInfo {
	public String task_id         ;                           
	public String notion_id       ;                           
	public String notion_type     ;                           
	public String notion_content  ;                           
	public String create_Time     ;                           
	public String create_User_Id  ;
	
	public String getTask_id() {
		return task_id;
	}
	public void setTask_id(String task_id) {
		this.task_id = task_id;
	}
	public String getNotion_id() {
		return notion_id;
	}
	public void setNotion_id(String notion_id) {
		this.notion_id = notion_id;
	}
	public String getNotion_type() {
		return notion_type;
	}
	public void setNotion_type(String notion_type) {
		this.notion_type = notion_type;
	}
	public String getNotion_content() {
		return notion_content;
	}
	public void setNotion_content(String notion_content) {
		this.notion_content = notion_content;
	}
	public String getCreate_Time() {
		return create_Time;
	}
	public void setCreate_Time(String create_Time) {
		this.create_Time = create_Time;
	}
	public String getCreate_User_Id() {
		return create_User_Id;
	}
	public void setCreate_User_Id(String create_User_Id) {
		this.create_User_Id = create_User_Id;
	}                           

	

}
