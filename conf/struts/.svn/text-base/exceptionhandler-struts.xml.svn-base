<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="exceptionhandler" extends="iscreate-struts-default">
		
		<!-- 跳转到错误页面的异常处理Action -->
		<action name="getErrorByExceptionHandlerAction" class="exceptionHandlerAction" method="getErrorByExceptionHandlerAction">
			<result name="error">/error.jsp</result>
		</action>
		
	</package>
</struts>
